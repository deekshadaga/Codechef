#include <iostream>
using namespace std;

int main() {
    int t;
    scanf("%d",&t);
    while(t--){
        int n,k,temp;
        scanf("%d %d",&n,&k);
        while(n--){
            scanf("%d",&temp);
            if(temp%k==0){
                printf("1");
            }
            else{
                printf("0");
            }
        }
        printf("\n");
    }
	return 0;
}
