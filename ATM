#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	float b;
	int a;
	cin>>a;
	cin>>fixed>>setprecision(2)>>b;
	if(a%5==0 && ((a+.50)<=b))
	{
	    b=b-a-0.50;
	    cout<<fixed<<setprecision(2)<<b;
	}
	else
	    cout<<fixed<<setprecision(2)<<b;
	
	return 0;
}
